    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@drawable/irregular_shape"
        android:src="@drawable/cart_push_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView119"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/mulish_regular"
        android:gravity="center"
        android:text="Added to cart!"
        android:textColor="#585858"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9"
        tools:layout_editor_absoluteX="53dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/check_out_btn"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="40dp"

        android:fontFamily="@font/mulish_regular"
        android:text="CheckOut"
        android:textAllCaps="false"
        android:background="@drawable/blue_box"
        android:backgroundTint="#F1F6F6"
        android:textColor="@color/colorBlue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView119" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/continue_btn"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/blue_box"
        android:fontFamily="@font/mulish_regular"
        android:text="Continue Shopping"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView119" />


</androidx.constraintlayout.widget.ConstraintLayout>